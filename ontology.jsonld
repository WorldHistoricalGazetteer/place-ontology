  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "\n        Links an Attestation to the Timespan during which the attested\n        properties (name, geometry, type) were applicable. Distinct from\n        the attestation's creation date: this is the historical timespan\n        being asserted, not when the assertion was made.\n    "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/plato#Attestation"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "attests timespan"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://w3id.org/plato#Timespan"
  } ]
}, {
