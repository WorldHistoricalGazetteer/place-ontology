  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "\n        Links an Attestation to a Type classification it assigns to the Thing.\n        An attestation may assert multiple types or no type at all.\n    "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
