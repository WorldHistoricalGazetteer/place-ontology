}, {
  "@id" : "https://w3id.org/plato#attests_about",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#FunctionalProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "\n        Links an Attestation to the Thing it is about. Every attestation must\n        have exactly one subject Thing. This is the primary entity being\n        described by the bundled name, geometry, timespan, and type assertions.\n\n        Functional property: each attestation has exactly one subject Thing.\n    "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/plato#Attestation"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "attests about"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://w3id.org/plato#Thing"
  } ]
}, {
  "@id" : "https://w3id.org/plato#attests_geometry",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "\n        Links an Attestation to a Geometry it asserts for the Thing. An\n        attestation may assert multiple geometries (e.g., point and polygon)\n        or no geometry at all (e.g., name-only attestations from textual sources).\n    "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/plato#Attestation"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "attests geometry"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://w3id.org/plato#Geometry"
  } ]
}, {
  "@id" : "https://w3id.org/plato#attests_name",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "\n        Links an Attestation to a Name it asserts for the Thing. An attestation\n        may assert multiple names (e.g., primary name and variants) or no name\n        at all (e.g., geometry-only attestations).\n    "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/plato#Attestation"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
