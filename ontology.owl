    <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#wktLiteral"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#Concept -->


    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
    


    <!-- http://www.w3.org/2006/time#ProperInterval -->


    <owl:Class rdf:about="http://www.w3.org/2006/time#ProperInterval"/>
    


    <!-- http://www.w3.org/ns/dcat#Dataset -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Dataset"/>
    


    <!-- http://www.w3.org/ns/prov#Agent -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Agent"/>
    


