#################################################################

###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/account
<http://xmlns.com/foaf/0.1/account> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/mbox
<http://xmlns.com/foaf/0.1/mbox> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/plato#attests_about
:attests_about rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Attestation ;
               rdfs:range :Thing ;
               rdfs:comment """
        Links an Attestation to the Thing it is about. Every attestation must
        have exactly one subject Thing. This is the primary entity being
        described by the bundled name, geometry, timespan, and type assertions.

        Functional property: each attestation has exactly one subject Thing.
    """@en ;
               rdfs:label "attests about"@en .


###  https://w3id.org/plato#attests_geometry
:attests_geometry rdf:type owl:ObjectProperty ;
                  rdfs:domain :Attestation ;
                  rdfs:range :Geometry ;
                  rdfs:comment """
        Links an Attestation to a Geometry it asserts for the Thing. An
        attestation may assert multiple geometries (e.g., point and polygon)
        or no geometry at all (e.g., name-only attestations from textual sources).
    """@en ;
                  rdfs:label "attests geometry"@en .


###  https://w3id.org/plato#attests_name
:attests_name rdf:type owl:ObjectProperty ;
              rdfs:domain :Attestation ;
              rdfs:range :Name ;
              rdfs:comment """
        Links an Attestation to a Name it asserts for the Thing. An attestation
        may assert multiple names (e.g., primary name and variants) or no name
        at all (e.g., geometry-only attestations).
    """@en ;
              rdfs:label "attests name"@en .


###  https://w3id.org/plato#attests_timespan
:attests_timespan rdf:type owl:ObjectProperty ;
                  rdfs:domain :Attestation ;
                  rdfs:range :Timespan ;
                  rdfs:comment """
        Links an Attestation to the Timespan during which the attested
        properties (name, geometry, type) were applicable. Distinct from
        the attestation's creation date: this is the historical timespan
        being asserted, not when the assertion was made.
    """@en ;
                  rdfs:label "attests timespan"@en .


###  https://w3id.org/plato#attests_type
:attests_type rdf:type owl:ObjectProperty ;
              rdfs:domain :Attestation ;
              rdfs:range :Type ;
              rdfs:comment """
        Links an Attestation to a Type classification it assigns to the Thing.
        An attestation may assert multiple types or no type at all.
    """@en ;
              rdfs:label "attests type"@en .


###  https://w3id.org/plato#candidate_candidate
:candidate_candidate rdf:type owl:ObjectProperty ;
                     rdfs:domain :Candidate ;
                     rdfs:range :Thing ;
                     rdfs:label "candidate thing"@en .


###  https://w3id.org/plato#candidate_source
:candidate_source rdf:type owl:ObjectProperty ;
                  rdfs:domain :Candidate ;
                  rdfs:range :Thing ;
                  rdfs:label "source thing"@en .


###  https://w3id.org/plato#contains_attestation
:contains_attestation rdf:type owl:ObjectProperty ;
                      rdfs:domain :Gazetteer ;
                      rdfs:range :Attestation ;
                      rdfs:comment "Links a Gazetteer to an Attestation contributed within it."@en ;
                      rdfs:label "contains attestation"@en .


###  https://w3id.org/plato#contains_thing
:contains_thing rdf:type owl:ObjectProperty ;
                rdfs:domain :Gazetteer ;
                rdfs:range :Thing ;
                rdfs:comment "Links a Gazetteer to a Thing it contains."@en ;
                rdfs:label "contains thing"@en .


###  https://w3id.org/plato#contributed_by
:contributed_by rdf:type owl:ObjectProperty ;
                rdfs:domain :Attestation ;
                rdfs:range :Contributor ;
                rdfs:comment """
        The agent (person, team, or process) who created this attestation.

        Semantically equivalent to PROV-O's prov:wasAttributedTo for provenance
        tracking. We don't use formal rdfs:subPropertyOf to avoid importing
        PROV-O's domain/range constraints. Distinct from sourced_by, which
        indicates the historical source being cited.
    """@en ;
                rdfs:label "contributed by"@en .


###  https://w3id.org/plato#contributor_affiliation
:contributor_affiliation rdf:type owl:ObjectProperty ;
                         rdfs:domain :Contributor ;
                         rdfs:range :Organization ;
                         rdfs:comment """
        Organizational affiliation of the contributor. Links to a
        plato:Organization entity, which can be further described using
        FOAF, schema.org, or other ontology properties. Organizations
        should ideally use persistent identifiers (ROR, ISNI, etc.).

        For simple cases, use contributor_affiliation_string for free-text
        affiliation names.
    """@en ;
                         rdfs:label "affiliation"@en .


###  https://w3id.org/plato#gazetteer_owner
:gazetteer_owner rdf:type owl:ObjectProperty ;
                 rdfs:domain :Gazetteer ;
                 rdfs:range :Contributor ;
                 rdfs:label "owner"@en .


###  https://w3id.org/plato#geo_wkt
:geo_wkt rdf:type owl:ObjectProperty ;
         rdfs:domain :Geometry ;
         rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  https://w3id.org/plato#has_meta_type
:has_meta_type rdf:type owl:ObjectProperty ;
               rdfs:domain :Attestation ;
               rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
               rdfs:comment """
        The nature of a meta-attestation's relationship to its target attestation.
        Should reference a controlled vocabulary concept defining the relationship
        type. Standard values include:

        - 'contradicts': asserts the target attestation is incorrect
        - 'supports': provides additional evidence for the target attestation
        - 'supersedes': replaces the target attestation with newer information
        - 'refines': provides more precise information than the target
        - 'challenges': questions the target attestation's validity
        - 'bundles': groups the target with other related attestations

        Using SKOS concepts rather than strings enables vocabulary management
        and multilingual labels.
    """@en ;
               rdfs:label "meta-attestation type"@en .


###  https://w3id.org/plato#has_relation_type
:has_relation_type rdf:type owl:ObjectProperty ;
                   rdfs:domain :Attestation ;
                   rdfs:range :RelationType ;
                   rdfs:comment """
        Specifies the semantic type of a Thing-to-Thing relationship
        attested by an Attestation. References a RelationType authority
        which defines the relationship's label, inverse, and domain/range
        constraints. Examples: 'capital_of', 'successor_to',
        'connected_by_trade_route_to'.

        The extensible vocabulary pattern allows new relationship types to
        be added as Authority records without modifying the ontology.
    """@en ;
                   rdfs:label "has relation type"@en .


###  https://w3id.org/plato#has_timespan
:has_timespan rdf:type owl:ObjectProperty ;
              rdfs:domain :Period ;
              rdfs:range :Timespan ;
              rdfs:comment """
        Links a Period authority to its temporal extent, represented as a
        Timespan node. Used for locally-defined periods or when more detailed
        temporal modeling is needed than provided by external period definitions.
    """@en ;
              rdfs:label "has timespan"@en .


###  https://w3id.org/plato#hull
:hull rdf:type owl:ObjectProperty ;
      rdfs:domain :Geometry ;
      rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  https://w3id.org/plato#identity_asserted_by
:identity_asserted_by rdf:type owl:ObjectProperty ;
                      rdfs:domain :IdentityRelation ;
                      rdfs:range :Contributor ;
                      rdfs:comment "The agent who made or confirmed this identity assertion."@en ;
                      rdfs:label "asserted by"@en .


###  https://w3id.org/plato#identity_object
:identity_object rdf:type owl:ObjectProperty ;
                 rdfs:domain :IdentityRelation ;
                 rdfs:range :Thing ;
                 rdfs:comment "The second Thing in an identity assertion."@en ;
                 rdfs:label "identity object"@en .


###  https://w3id.org/plato#identity_sourced_by
:identity_sourced_by rdf:type owl:ObjectProperty ;
                     rdfs:domain :IdentityRelation ;
                     rdfs:range :Authority ;
                     rdfs:comment """
        The Authority (Source or Dataset) that provides evidence for
        this identity assertion, if any.
    """@en ;
                     rdfs:label "identity sourced by"@en .


###  https://w3id.org/plato#identity_subject
:identity_subject rdf:type owl:ObjectProperty ;
                  rdfs:domain :IdentityRelation ;
                  rdfs:range :Thing ;
                  rdfs:comment "The first Thing in an identity assertion."@en ;
                  rdfs:label "identity subject"@en .


###  https://w3id.org/plato#member_of_group
:member_of_group rdf:type owl:ObjectProperty ;
                 rdfs:domain :Gazetteer ;
                 rdfs:range :GazetteerGroup ;
                 rdfs:comment "Links a Gazetteer to a GazetteerGroup."@en ;
                 rdfs:label "member of group"@en .


###  https://w3id.org/plato#meta_attestation_about
:meta_attestation_about rdf:type owl:ObjectProperty ;
                        rdfs:domain :Attestation ;
                        rdfs:range :Attestation ;
                        rdfs:comment """
        Links a meta-Attestation to the Attestation it comments on or relates to.
        Used with plato:has_meta_type to specify the nature of the relationship.

        Example: A meta-attestation might assert that one source's geometry
        attestation contradicts another source's geometry attestation for the
        same Thing.
    """@en ;
                        rdfs:label "meta-attestation about"@en .


###  https://w3id.org/plato#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :Source ;
        rdfs:range :Dataset ;
        rdfs:comment "Links a Source to its parent Dataset."@en ;
        rdfs:label "part of"@en .


###  https://w3id.org/plato#promoted_from
:promoted_from rdf:type owl:ObjectProperty ;
               rdfs:domain :IdentityRelation ;
               rdfs:range :Candidate ;
               rdfs:comment """
        Links an IdentityRelation to the Candidate from which it was
        promoted, preserving the algorithmic provenance. Optional:
        identity relations can also be created directly by human
        assertion without a prior Candidate.
    """@en ;
               rdfs:label "promoted from"@en .


###  https://w3id.org/plato#relates_to
:relates_to rdf:type owl:ObjectProperty ;
            rdfs:domain :Attestation ;
            rdfs:range :Thing ;
            rdfs:comment """
        Links an Attestation to a second Thing, asserting a relationship
        between the Attestation's subject Thing (via attests_about) and this
        target Thing. The semantic nature of the relationship is specified by
        plato:has_relation_type, which references a RelationType authority.

        Example: An attestation about Constantinople that relates_to the
        Byzantine Empire with relation_type \"capital_of\" asserts that
        \"Constantinople was the capital of the Byzantine Empire\".
    """@en ;
            rdfs:label "relates to"@en .


###  https://w3id.org/plato#relative_qualifier
:relative_qualifier rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Geometry
                                                :Name
                                                :Timespan
                                                :Type
                                              )
                                ] ;
                    rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                    rdfs:comment """
        A controlled-vocabulary term describing the relative relationship
        when bearing and distance are not applicable or not sufficient.
        Should point to a concept in a SKOS concept scheme (e.g.
        plato:Near, plato:Within, plato:Beyond, plato:UpstreamOf,
        plato:BetweenXAndY, plato:DuringReignOf, plato:VariantOf).
        Using URIs rather than free text enables machine reconciliation
        across gazetteers that describe relativity differently.
    """@en ;
                    rdfs:label "relative qualifier"@en .


###  https://w3id.org/plato#relative_to
:relative_to rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Geometry
                                         :Name
                                         :Timespan
                                         :Type
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Name
                                        :Period
                                        :Thing
                                        :Timespan
                                        :Type
                                      )
                        ] ;
             rdfs:comment """
        Links a facet node to its anchor entity, indicating that this
        facet is defined relationally rather than absolutely. The anchor
        is typically a Thing, Period, or another facet (Name, Timespan, etc.).

        Common patterns:
        - Spatial relativity: \"20km east of Nanjing\" (anchor is a Thing)
        - Temporal relativity: \"during the reign of Justinian\" (anchor is a Period)
        - Name relativity: \"also known as Rome\" (anchor is another Name)
    """@en ;
             rdfs:label "relative to"@en .


###  https://w3id.org/plato#repr_point
:repr_point rdf:type owl:ObjectProperty ;
            rdfs:domain :Geometry ;
            rdfs:range <http://www.opengis.net/ont/geosparql#wktLiteral> .


###  https://w3id.org/plato#sourced_by
:sourced_by rdf:type owl:ObjectProperty ;
            rdfs:domain :Attestation ;
            rdfs:range :Authority ;
            rdfs:comment """
        Links an Attestation to the Authority (typically a Source or Dataset)
        that grounds it. This provides the provenance chain for the attestation.

        Semantically equivalent to PROV-O's prov:wasDerivedFrom, indicating the
        authority is the origin of the attested knowledge. We don't use formal
        rdfs:subPropertyOf to avoid importing PROV-O's domain/range constraints
        which conflict with PLATO's class structure.
    """@en ;
            rdfs:label "sourced by"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.opengis.net/ont/geosparql#asWKT
<http://www.opengis.net/ont/geosparql#asWKT> rdf:type owl:DatatypeProperty .


###  https://w3id.org/plato#algorithm_version
:algorithm_version rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Candidate ;
                   rdfs:range xsd:string ;
                   rdfs:label "algorithm version"@en .


###  https://w3id.org/plato#authority_title
:authority_title rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Authority ;
                 rdfs:range xsd:string ;
                 rdfs:label "title"@en .


###  https://w3id.org/plato#authority_uri
:authority_uri rdf:type owl:DatatypeProperty ;
               rdfs:domain :Authority ;
               rdfs:range xsd:anyURI ;
               rdfs:label "authority URI"@en .


###  https://w3id.org/plato#authority_version
:authority_version rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Authority ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Version identifier for tracking upstream authority updates."@en ;
                   rdfs:label "version"@en .


###  https://w3id.org/plato#bbox
:bbox rdf:type owl:DatatypeProperty ;
      rdfs:domain :Geometry ;
      rdfs:range xsd:string ;
      rdfs:comment """
        Bounding box as array: [min_lon, min_lat, max_lon, max_lat].
        Enables fast spatial filtering. Format: comma-separated string
        or JSON array depending on serialization context.
    """@en ;
      rdfs:label "bounding box"@en .


###  https://w3id.org/plato#bibliographic_string
:bibliographic_string rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Source ;
                      rdfs:range xsd:string ;
                      rdfs:comment "A formatted citation string for this source."@en ;
                      rdfs:label "bibliographic string"@en .


###  https://w3id.org/plato#candidate_status
:candidate_status rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Candidate ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Review status: 'suggested', 'confirmed', 'rejected'."@en ;
                  rdfs:label "status"@en .


###  https://w3id.org/plato#ccodes
:ccodes rdf:type owl:DatatypeProperty ;
        rdfs:domain :Thing ;
        rdfs:range xsd:string ;
        rdfs:comment "ISO 3166-1 alpha-2 country codes associated with this Thing."@en ;
        rdfs:label "country codes"@en .


###  https://w3id.org/plato#certainty
:certainty rdf:type owl:DatatypeProperty ;
           rdfs:domain :Attestation ;
           rdfs:range xsd:float ;
           rdfs:comment """
        Overall certainty of this attestation, from 0.0 (completely uncertain)
        to 1.0 (completely certain). This represents the contributor's
        confidence in the bundle as a whole. Individual facets (Name, Geometry,
        Timespan) can have their own uncertainty values that may differ from
        the attestation's overall certainty.
    """@en ;
           rdfs:label "certainty"@en .


###  https://w3id.org/plato#certainty_note
:certainty_note rdf:type owl:DatatypeProperty ;
                rdfs:domain :Attestation ;
                rdfs:range xsd:string ;
                rdfs:comment """
        Free-text explanation of the certainty assessment. Should describe
        the basis for the certainty value: quality of evidence, degree of
        scholarly consensus, reliability of source, etc.
    """@en ;
                rdfs:label "certainty note"@en .


###  https://w3id.org/plato#connection_metadata
:connection_metadata rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Attestation ;
                     rdfs:range xsd:string ;
                     rdfs:comment """
        Structured metadata for network-type attestations. Can encode
        information about trade goods, flow direction, capacity, frequency,
        or other properties of network connections. Format is implementation-
        specific (JSON recommended) but should be documented in the gazetteer's
        metadata.
    """@en ;
                     rdfs:label "connection metadata"@en .


###  https://w3id.org/plato#contributor_affiliation_string
:contributor_affiliation_string rdf:type owl:DatatypeProperty ;
                                rdfs:domain :Contributor ;
                                rdfs:range xsd:string ;
                                rdfs:comment """
        Free-text organizational affiliation when a structured Organization
        entity is not available. Example: \"University of Pittsburgh,
        Institute for Spatial History Innovation\"
    """@en ;
                                rdfs:label "affiliation (string)"@en .


###  https://w3id.org/plato#contributor_email
:contributor_email rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Contributor ;
                   rdfs:range xsd:string ;
                   rdfs:comment """
        Contact email address for the contributor. Useful for collaboration
        and correspondence about contributed data.
    """@en ;
                   rdfs:label "email"@en .


###  https://w3id.org/plato#contributor_identifier
:contributor_identifier rdf:type owl:DatatypeProperty ;
                        rdfs:domain :Contributor ;
                        rdfs:range xsd:string ;
                        rdfs:comment """
        Additional persistent identifier for the contributor. Can be used
        for identifiers other than ORCID, such as:
        - ISNI (International Standard Name Identifier)
        - VIAF (Virtual International Authority File)
        - Wikidata QID (for persons or organizations)
        - Internal system identifiers

        When using this property, specify the identifier type in the value
        (e.g., \"ISNI:0000000121032683\" or \"VIAF:12345678\").
    """@en ;
                        rdfs:label "contributor identifier"@en .


###  https://w3id.org/plato#contributor_name
:contributor_name rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Contributor ;
                  rdfs:range xsd:string ;
                  rdfs:comment """
        The name of the contributor (person, team, or automated process).
        For persons, should be the full name as preferred by the contributor.
        For teams, the team name. For automated processes, a descriptive name.
    """@en ;
                  rdfs:label "contributor name"@en .


###  https://w3id.org/plato#contributor_orcid
:contributor_orcid rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Contributor ;
                   rdfs:range xsd:string ;
                   rdfs:comment """
        ORCID identifier for person contributors. Can be provided in two formats:

        1. Compact format: 0000-0000-0000-0000 (sixteen digits in groups of four)
        2. URI format: https://orcid.org/0000-0000-0000-0000 (preferred)

        The URI format (https://orcid.org/...) is preferred and required for
        proper linked data representation. ORCID is particular about always
        including the https:// protocol and domain prefix.

        While optional in the general ontology, specific implementations
        (e.g., WHG) may require ORCID for person contributors to ensure
        proper attribution and citation.

        ORCID provides persistent identification for researchers, enabling
        proper credit and reducing name ambiguity.
    """@en ;
                   rdfs:label "ORCID"@en .


###  https://w3id.org/plato#contributor_role
:contributor_role rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Contributor ;
                  rdfs:range xsd:string ;
                  rdfs:comment """
        Role of the contributor in the project or gazetteer. Examples:
        'principal_investigator', 'data_curator', 'student_researcher',
        'automated_process', 'external_contributor'. Helps understand the
        context and authority of contributions.
    """@en ;
                  rdfs:label "contributor role"@en .


###  https://w3id.org/plato#contributor_url
:contributor_url rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Contributor ;
                 rdfs:range xsd:anyURI ;
                 rdfs:comment """
        Website or profile URL for the contributor (personal website,
        institutional page, project page, GitHub profile, etc.). Provides
        additional context about the contributor's work and identity.
    """@en ;
                 rdfs:label "contributor URL"@en .


###  https://w3id.org/plato#created
:created rdf:type owl:DatatypeProperty ;
         rdfs:domain :Attestation ;
         rdfs:range xsd:dateTime ;
         rdfs:comment """
        Timestamp when this attestation was created. This is the contribution
        date, not the historical timespan being attested (use attests_timespan
        for that).

        Semantically equivalent to PROV-O's prov:generatedAtTime. We don't use
        formal rdfs:subPropertyOf to avoid importing PROV-O constraints.
    """@en ;
         rdfs:label "created"@en .


###  https://w3id.org/plato#edtf_string
:edtf_string rdf:type owl:DatatypeProperty ;
             rdfs:domain :Timespan ;
             rdfs:range xsd:string ;
             rdfs:comment """
        Extended Date/Time Format (EDTF) representation of this timespan.
        Provides standardized string encoding for interchange with systems
        using EDTF. Optional but recommended for interoperability with library
        and archival systems. Example: '1453-05-29/1453-05-29' or '1450~/1453~'
        for approximate dates.
    """@en ;
             rdfs:label "EDTF string"@en .


###  https://w3id.org/plato#end_earliest
:end_earliest rdf:type owl:DatatypeProperty ;
              rdfs:domain :Timespan ;
              rdfs:range xsd:string ;
              rdfs:comment """
        The earliest possible end date for this timespan. Together with
        end_latest, defines the uncertainty range for when the interval ended.
        May be null for unknown or deliberately open-end intervals.
    """@en ;
              rdfs:label "end earliest"@en .


###  https://w3id.org/plato#end_latest
:end_latest rdf:type owl:DatatypeProperty ;
            rdfs:domain :Timespan ;
            rdfs:range xsd:string ;
            rdfs:comment """
        The latest possible end date for this timespan. For certain dates,
        this equals end_earliest. For uncertain dates, defines the end of
        the uncertainty range for when the interval ended.
    """@en ;
            rdfs:label "end latest"@en .


###  https://w3id.org/plato#end_precision
:end_precision rdf:type owl:DatatypeProperty ;
               rdfs:domain :Timespan ;
               rdfs:range xsd:string ;
               rdfs:comment """
        Temporal granularity of the end bound. Values: 'year', 'decade',
        'century', 'era', 'geological_period'.
    """@en ;
               rdfs:label "end precision"@en .


###  https://w3id.org/plato#fuzziness
:fuzziness rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:float ;
           rdfs:comment """
        Ontological fuzziness: a measure of how inherently vague or
        indeterminate the facet's referent is, from 0.0 (crisp, sharply
        bounded) to 1.0 (maximally vague). Distinct from uncertainty:
        fuzziness is a property of the thing itself, not of our knowledge
        of it. A regional boundary that was never precisely defined has
        high fuzziness regardless of how much evidence we have.
    """@en ;
           rdfs:label "fuzziness"@en .


###  https://w3id.org/plato#fuzziness_note
:fuzziness_note rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                rdfs:comment """
        Free-text explanation of the nature of ontological fuzziness
        (e.g. 'regional boundary was never formally demarcated',
        'name used inconsistently across contemporary sources',
        'transition was gradual rather than a discrete event').
    """@en ;
                rdfs:label "fuzziness note"@en .


###  https://w3id.org/plato#gazetteer_status
:gazetteer_status rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Gazetteer ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Publication status: 'draft' or 'published'."@en ;
                  rdfs:label "status"@en .


